{
  "data_loader_config": {
    "infer_datetime": true,
    "datetime_format": "%b %d %H:%M:%S",
    "reader_args": {
      "log_format": "<DateTime> <project> <kerneltime> <Level>,<Content>"
    },
    "dimensions": {
      "timestamp": ["DateTime"],
      "body": ["Content"],
      "attributes": ["Level"]
    }
  },
  "preprocessor_config": {
    "custom_delimiters_regex":[],
    "custom_replace_list": {
      "^([A-Za-z]{3}\\s+[0-9]{1,2}\\s+[0-9]{2}:[0-9]{2}:[0-9]{2})": "<DateTime>",
      "(WiFi|7992)\\@([\\w]+)": "<Level>"
    }
  },
   "log_parser_config": {
    "custom_filter_regex": [
      "^[A-Za-z]{3}\\s+[0-9]{1,2}\\s+[0-9]{2}:[0-9]{2}:[0-9]{2}"
    ],
    "parsing_algorithm": "drain",
    "parsing_algo_params": {
      "sim_th": 0.5,
      "depth": 5
    }
  },
    "feature_extractor_config": {
      "group_by_category": ["Level"],
      "group_by_time": "1min"
  },
  "log_vectorizer_config": {
          "algo_name": "word2vec"
  },
  "categorical_encoder_config": {
      "name": "label_encoder"
  },
  "anomaly_detection_config": {
      "algo_name": "one_class_svm"
  }
}
